{
    "name": "naraki/components",
    "description": "Components for the Naraki platform",
    "keywords": ["laravel", "naraki"],
    "license": "MIT",
    "authors": [
        {
            "name": "Bruno D.C",
            "email": "dev.naraki.io@gmail.com"
        }
    ],
    "require": {
      "cyrildewit/eloquent-viewable": "^3.2",
      "elasticsearch/elasticsearch": "^6.1",
      "google/apiclient": "^2.2",
      "laravel/socialite": "^4.1",
      "laravolt/avatar": "^2.2",
      "mailgun/mailgun-php": "^2.8.1",
      "mews/purifier": "^2.1",
      "ongr/elasticsearch-dsl": "^6.0",
      "php-http/guzzle6-adapter": "^1.1",
      "php-http/message": "^1.7.2",
      "ramsey/uuid": "^3.8",
      "suin/php-rss-writer": "^1.6",
      "thepixeldeveloper/sitemap": "^5.1"
    },
    "autoload": {
      "psr-4": {
        "Naraki\\": "src/"
      },
      "files": [
        "src/core/helpers.php"
      ]
    },
    "replace": {
        "naraki/blog": "0.1.0",
        "naraki/core": "0.1.0",
        "naraki/elasticsearch": "self.version",
        "naraki/forum": "0.1.0",
        "naraki/mail": "0.1.0",
        "naraki/media": "0.1.0",
        "naraki/oauth": "0.1.0",
        "naraki/permission": "0.1.0",
        "naraki/rss": "0.1.0",
        "naraki/sentry": "0.1.0",
        "naraki/shop": "0.1.0",
        "naraki/sitemap": "0.1.0",
        "naraki/system": "0.1.0"
    },
    "config": {
      "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
